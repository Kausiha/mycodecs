package guvi;

public class UniqueElementInArray {
	public static void main(String [] args){
		int [] a=new int[5];
		a[0]=6;
		a[1]=2;
		a[2]=2;
		a[3]=6;
		a[4]=5;
		int c=0;
		for(int i=0;i<a.length;i++){
			for(int j=i+1;j<a.length;j++){
				if(a[i]==a[j])
					c++;
			}
			if(c==0)
			{
				System.out.print(a[i]);
				break;
			}
			c=0;
		}
		
	}

}
